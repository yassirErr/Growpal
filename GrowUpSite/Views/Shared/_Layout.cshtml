    <!-- Meanmenu JS -->
    <script src="~/site/assets/js/jquery.meanmenu.js"></script>
    <!-- Popup JS -->
    <script src="~/site/assets/js/jquery.magnific-popup.min.js"></script>
    <!-- Odometer JS -->
    <script src="~/site/assets/js/odometer.min.js"></script>
    <script src="~/site/assets/js/jquery.appear.js"></script>
    <!-- Owl Carousel JS -->
    <script src="~/site/assets/js/owl.carousel.min.js"></script>
    <!-- Thumb Slider JS -->
    <script src="~/site/assets/js/thumb-slide.js"></script>
    <!-- Wow JS -->
    <script src="~/site/assets/js/wow.min.js"></script>
    <!-- Custom JS -->
    <script src="~/site/assets/js/custom.js"></script>
    @await RenderSectionAsync("Scripts", required: false)

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
